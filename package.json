{
  "name": "aragon-voting",
  "version": "0.1.0",
  "scripts": {
    "build-manifest": "scripts/build-manifest.sh rinkeby",
    "create": "graph create ajsantander/aragon-voting --node https://api.thegraph.com/deploy/",
    "create-local": "graph create ajsantander/aragon-voting --node http://127.0.0.1:8020",
    "codegen": "rm -rf generated; graph codegen",
    "build": "graph build",
    "deploy": "graph deploy ajsantander/aragon-voting --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --access-token $GRAPHKEY",
    "deploy-local": "graph deploy ajsantander/aragon-voting --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "mustache": "^4.0.1",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
